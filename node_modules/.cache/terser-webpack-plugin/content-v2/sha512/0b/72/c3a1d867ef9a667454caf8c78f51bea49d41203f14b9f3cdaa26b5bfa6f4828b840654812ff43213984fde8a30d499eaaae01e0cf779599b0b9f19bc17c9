\n\n        if (json === null)\n            return [];\n\n        let jsonParsed = JSON.parse(json, (key, value) => {\n            if (key === \"date\") {\n                value = new Date(value);\n            }\n            return value;\n        });\n\n        if (jsonParsed.length === 0)\n            return [];\n\n        let addNewItems = [];\n\n        for (let i = 0; i < jsonParsed.length; i++) {\n            addNewItems.push(AddNewItem.fromJSON(jsonParsed[i]));\n        }\n\n        return addNewItems;\n\n    }\n\n}\n\n\nexport default AddNew;\n","import React from 'react';\nimport './App.css';\nimport Header from './components/Header/Header.js';\nimport Body from './components/Body/Body.js';\nimport Footer from './components/Footer/Footer.js';\n\nimport AddNewItem from './model/AddNewItem';\nimport AddNew from './model/AddNew';\n\nclass App extends React.Component {\n\n  constructor(props) {\n  super(props);\n\n  this.phonebook = new AddNew();\n\n  this.state = {\n    activeTab: -1,\n    items: this.phonebook.items\n  };\n\n  this.createItem = this.createItem.bind(this);\n  this.deleteItem = this.deleteItem.bind(this);\n\n  }\n\n  componentDidMount() {\n    window.onpopstate = this.examinePath;\n  }\n\n  createItem(firstname, lastname, number) {\n    let item = new AddNewItem(firstname, lastname, number, new Date(),);\n    let items = this.phonebook.add(item);\n    this.setState({items: items});\n\n  }\n\n  deleteItem(item) {\n    let items = this.phonebook.delete(item);\n    this.setState({ items: items });\n  }\n\n  render () {\n      return (<div id=\"app-container\">\n      <Header></Header>\n      <Body items={this.state.items} onDeleteItem={this.deleteItem}></Body>\n      <Footer createItem={this.createItem}></Footer>\n      </div>);\n  }\n}\n\nexport default App;\n","const reportWebVitals = onPerfEntry => {\n  if (onPerfEntry && onPerfEntry instanceof Function) {\n    import('web-vitals').then(({ getCLS, getFID, getFCP, getLCP, getTTFB }) => {\n      